<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="false" android:state_pressed="true">
        <shape>
            <corners android:radius="@dimen/btn_radius_4dp" />
            <solid android:color="@color/migration_checkbox_checked_background" />
            <stroke android:width="1.1999817dip" android:color="@color/migration_checkbox_checked_border" />
        </shape>
    </item>
    <item android:state_focused="true" android:state_checked="false">
        <shape>
            <corners android:radius="@dimen/btn_radius_4dp" />
            <solid android:color="@color/migration_checkbox_checked_background" />
            <stroke android:width="1.1999817dip" android:color="@color/migration_checkbox_checked_border" />
        </shape>
    </item>
    <item android:state_checked="false">
        <shape>
            <corners android:radius="@dimen/btn_radius_4dp" />
            <stroke android:width="0.8000001dip" android:color="@color/migration_checkbox_unchecked_border" />
        </shape>
    </item>
    <item android:state_checked="true">
        <shape>
            <corners android:radius="@dimen/btn_radius_4dp" />
            <solid android:color="@color/migration_checkbox_checked_background" />
            <stroke android:width="1.1999817dip" android:color="@color/migration_checkbox_checked_border" />
        </shape>
    </item>
</selector>